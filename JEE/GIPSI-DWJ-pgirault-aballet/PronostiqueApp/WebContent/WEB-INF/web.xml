<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>PGWebChat</display-name>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
  <listener>
    <listener-class>fr.emac.gipsi.gsi.config.InitialisationXML</listener-class>
  </listener>
  <welcome-file-list>
    <welcome-file>login</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ServletLogin</servlet-name>
    <servlet-class>fr.emac.gipsi.gsi.servlets.ServletLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletLogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletMessage</servlet-name>
    <servlet-class>fr.emac.gipsi.gsi.servlets.ServletMessage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletMessage</servlet-name>
    <url-pattern>/messages</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletInscription</servlet-name>
    <servlet-class>fr.emac.gipsi.gsi.servlets.ServletInscription</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletInscription</servlet-name>
    <url-pattern>/inscription</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletMatchs</servlet-name>
    <servlet-class>fr.emac.gipsi.gsi.servlets.ServletMatchs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletMatchs</servlet-name>
    <url-pattern>/matchs</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletPronostique</servlet-name>
    <servlet-class>fr.emac.gipsi.gsi.servlets.ServletPronostique</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletPronostique</servlet-name>
    <url-pattern>/pronostiquer</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletLogout</servlet-name>
    <servlet-class>fr.emac.gipsi.gsi.servlets.ServletLogout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletLogout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>